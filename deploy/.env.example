# AWS credentials for EC2 + S3
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1

# S3 bucket for Zeppelin data (must exist in us-east-1)
S3_BUCKET=your-zeppelin-bench-bucket

# EC2 SSH key pair name (must be registered in EC2)
EC2_KEY_NAME=your-key-pair-name

# Path to private key file for SSH
EC2_KEY_FILE=~/.ssh/your-key.pem

# Instance type (default: c7i.xlarge — 4 vCPU, 8GB, ~$0.17/hr)
EC2_INSTANCE_TYPE=c7i.xlarge

# AMI (Amazon Linux 2023, us-east-1 — update if using different region)
EC2_AMI=al2023-ami-latest

# Grafana admin password (used by monitoring stack)
# Generate one: openssl rand -base64 24 | tr -d '/+=' | head -c 20
GRAFANA_ADMIN_PASSWORD=changeme
